{
  "name": "Hue Bridge",
  "version":2.1,
  "driverType": "RestDriver",
  "actions":{
    "getAllLights":{
      "multiURL":false,
      "commands":[
        {
          "requestType": "GET",
          "url": "http://<ip>/api/<authId>/lights"
        }
      ]   
    },
    "getNewLights":{
      "multiURL":true,
      "commands":[
        {
          "requestType": "POST",
          "url": "http://<ip>/api/<authId>/lights"
        },
        {
          "requestType": "GET",
          "url": "http://<ip>/api/<authId>/lights/new"
        }
      ]
      
    },
    "getAllSensors":{
      "multiURL":false,
      "commands":[
        {
          "requestType": "GET",
          "url": "http://<ip>/api/<authId>/sensors"
        }
      ]
    }
  }

}
  